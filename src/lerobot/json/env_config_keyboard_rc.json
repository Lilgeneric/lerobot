{
  "env": {
    "name": "real_robot",
    "fps": 10,
    "processor": {
      "control_mode": "keyboard",
      "observation": {
        "display_cameras": false,
        "add_joint_velocity_to_observation": true,
        "add_current_to_observation": true
      },
      "image_preprocessing": {
        "crop_params_dict": {
          "observation.images.top": [76, 84, 248, 448],
          "observation.images.wrist": [0, 0, 480, 640],
          "observation.images.side": [97, 27, 299, 612]

        },
        "resize_size": [128, 128]
      },
      "gripper": {
        "use_gripper": true,
        "gripper_penalty": 0.0
      },
      "reward_classifier": {
        "pretrained_path": "/home/generic/lerobot/outputs/train/2025-12-09/01-09-27_reward-classifier/checkpoints/last/pretrained_model",
        "success_threshold": 0.7,
        "success_reward": 1.0
      },
      "reset": {
        "reset_time_s": 5.0,
        "control_time_s": 120.0,
        "fixed_reset_joint_positions": [3.1833, -84.0739, 65.8245, 72.1586, -0.1709, 1.6656],
        "terminate_on_success": true
      },

      "inverse_kinematics": {
        "urdf_path": "/home/generic/lerobot/Simulation/SO101/so101_new_calib.urdf",
        "target_frame_name": "gripper_frame_link",
        "end_effector_bounds": {
          "min": [0.0084, -0.1797, -0.1024],
          "max": [0.3647, 0.1749, 0.2556]
        },
        "end_effector_step_sizes": {
          "x": 0.005,
          "y": 0.005,
          "z": 0.005
        }
      }
    },
    "robot": {
      "type": "so100_follower",
      "port": "/dev/ttyACM1",
      "id": "my_awesome_follower_arm",
      "use_degrees": true,
      "cameras": {
        "top": {
          "type": "opencv",
          "index_or_path": 0,
          "height": 480,
          "width": 640,
          "fps": 30
        },
        "wrist": {
          "type": "opencv",
          "index_or_path": 6,
          "height": 480,
          "width": 640,
          "fps": 30
        },
        "side": {
          "type": "opencv",
          "index_or_path": 4,
          "height": 480,
          "width": 640,
          "fps": 30
        }
      }
    },
    "teleop": {
      "type": "keyboard_ee",
      "use_gripper": true
    }
  },
  "dataset": {
    "repo_id": "place/reward_classifier_test",
    "root": null,
    "task": "Move the object inside the yellow area",
    "num_episodes_to_record": 40,
    "replay_episode": 0,
    "push_to_hub": false
  },
  "mode": null,
  "device": "cuda"
}